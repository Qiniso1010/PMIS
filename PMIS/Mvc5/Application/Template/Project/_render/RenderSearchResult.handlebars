<div class="col-lg-12">
  <h2>Items</h2>
  <table class="table table-responsive no-padding no-border table-hover">
    <tbody>
      {{#each projects}}
        <tr>
          <td>
            <a href="javascript:;" id="btnDetail" title="Click for detail." data-id="{{Id}}">
              {{Name}}
            </a>
            <label class="label bg-blue r-2">{{ProjectName}}</label>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<div class="col-lg-12">
  <h2>Lampiran</h2>
  <table class="table table-responsive no-padding no-border table-hover">
    <tbody>
      {{#each files}}
        <tr>
          <td>
            <a href="/Uploads/{{Name}}" target="_blank" title="Download this file." data-id="{{Id}}">
              {{#ifNull RealName}}
                {{Name}}
              {{else}}
                {{RealName}}
              {{/ifNull}}
            </a>
            <label class="label bg-blue r-2">{{ProjectName}}</label>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
